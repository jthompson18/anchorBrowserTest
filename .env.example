# ===== Anchor Browser (required) =====
ANCHOR_API_KEY=anc_sk_your_api_key_here

# ===== Search API (required) =====
SERPER_API_KEY=serper_your_key_here

# ===== Gemini (optional: model writes the paper text) =====
GEMINI_API_KEY=your_gemini_or_google_api_key_here
# GEMINI_TEXT_MODEL can be gemini-1.5-flash (fast) or gemini-1.5-pro (stronger)
GEMINI_TEXT_MODEL=gemini-1.5-flash
USE_GEMINI_TEXT_GEN=true

# ===== Session knobs (optional) =====
KEEP_SESSION_ON_SUCCESS=false
CLOSE_ON_ERROR=true
